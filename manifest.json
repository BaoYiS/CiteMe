{
  "manifest_version": 3,
  "name": "CiteMe",
  "description": "Citation Generator for CiteCoin",
  "version": "0.1.0",
  "icons": {
    "128": "128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs"
  ]
}
